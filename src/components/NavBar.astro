---
import NavLink from "./NavLink.astro";
import ShopNow from "./ShopNow.astro";
const links = [
    {url: "/", text: "Home"},
    {url: "/shop", text: "Shop"},
    {url: "/services", text: "Services"},
    {url: "/about", text: "About"},
    {url: "/contact", text: "Contact"},
    {url: "/blog", text: "Blog"},
]
---

<div class="">
    <div class="container dorik-row-navbar-wrapper ">
        <div class="dorik-row-navbar  flex  ">
            <div class="col-lg-1/1 ">
                <div class="column-inner dorik-column-navbar-brand  1/1">
                    <div class="dorik-navbar--wrapper dorik-nav-wrapper  ">
                        <div class="dorik-navbar dorik-nav-navbar " id="blog">
                            <div class="dorik-navbar--brand">
                                <a href=""><img
                                        src="https://cdn.cmsfly.com/6534e3e885f4bd0012ee789d/logoipsum-2632-z3M_Xh.png"
                                        alt="Logo" width="200px"></a>
                            </div>
                            <button aria-label="Navbar Toggle" class="dorik-navbar--toggle"
                                    data-target="#dorikNavbarCollapse">
                                <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="bars"
                                     class="svg-inline--fa fa-bars fa-fw " role="img"
                                     xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512">
                                    <path fill="currentColor"
                                          d="M0 96C0 78.3 14.3 64 32 64H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32C14.3 128 0 113.7 0 96zM0 256c0-17.7 14.3-32 32-32H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32c-17.7 0-32-14.3-32-32zM448 416c0 17.7-14.3 32-32 32H32c-17.7 0-32-14.3-32-32s14.3-32 32-32H416c17.7 0 32 14.3 32 32z"></path>
                                </svg>
                            </button>
                            <div class="dorik-navbar--collapse collapse" id="dorikNavbarCollapse">
                                <div class="dorik-navbar--navs">
                                    <ul class="dorik-nav--links normalize">
                                        {
                                            links.map((link) => {
                                                return <NavLink text={link.text} url={link.url} />
                                            })
                                        }
                                    </ul>
                                    <ShopNow text="Shop Now" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>